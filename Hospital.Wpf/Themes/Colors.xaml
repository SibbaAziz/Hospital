<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Color1">#3722f6</Color>
    <Color x:Key="Color2">#b39afd</Color>
    <Color x:Key="Color3">#FFD54F</Color>
    <Color x:Key="BackgroudColor">White</Color>
    <SolidColorBrush x:Key="Brush1" Color="{StaticResource Color1}"/>
    <SolidColorBrush x:Key="Brush2" Color="{StaticResource Color2}"/>
    <SolidColorBrush x:Key="Brush3" Color="{StaticResource Color3}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroudColor}"/>

</ResourceDictionary>